#! /bin/sh

echo "Building Cypress Client"
docker build --build-arg UID=$(id -u) --build-arg GID=$(id -g) -f docker/Dockerfile.cypress -t syntapynew/cypress_client ./gatsby/

echo
echo
echo
echo "Building Web Server"
docker build --build-arg UID=$(id -u) --build-arg GID=$(id -g) -f docker/Dockerfile.gatsby -t syntapynew/gatsby_server ./gatsby/
