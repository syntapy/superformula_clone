#! /bin/sh

echo "Building Cypress Client"
docker build --build-arg UID=1000 --build-arg GID=1000 -f docker/Dockerfile.cypress -t syntapynew/cypress_client ./gatsby/

echo
echo
echo
echo "Building Web Server"
docker build --build-arg UID=1000 --build-arg GID=1000 -f docker/Dockerfile.gatsby -t syntapynew/gatsby_server ./gatsby/
