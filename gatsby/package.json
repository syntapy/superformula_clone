{
  "name": "superformula-lp-clone",
  "version": "1.0.0",
  "private": true,
  "description": "superformula-lp-clone",
  "author": "Sami Abdul-Wahid",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "serve:help": "gatsby serve --help",
    "setup": "node ./bin/setup",
    "info": "gatsby info",
    "clean": "gatsby clean",
    "help": "gatsby --help",
    "which": "which gatsby",
    "test": "jest",
    "test help": "jest --help",
    "cy:install": "cypress install",
    "cy:open": "cypress open",
    "cy:help": "cypress run --help",
    "audit": "cypress run --e2e --browser firefox --spec 'cypress/audit/**/*.cy.js'",
    "audit:ci": "start-server-and-test develop http://localhost:8000 audit",
    "audit:debug": "DEBUG='cypress:server:task' cypress run --e2e --browser firefox --spec 'cypress/audit/**/*.cy.js'",
    "e2e": "cypress run --e2e --browser firefox --spec 'cypress/e2e/**/*.cy.js'",
    "e2e:ci": "start-server-and-test develop http://localhost:8000 e2e",
    "e2e:reset": "cypress run --env type=base --e2e --browser firefox",
    "e2e:view:mobile": "sh cypress/bin/compare_images",
    "e2e:view:desktop": "sh cypress/bin/compare_images --desktop",
    "e2e:clear": "sh cypress/bin/compare_images --clear",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@cypress/webpack-preprocessor": "^6.0.2",
    "dotenv": "^16.4.5",
    "gatsby": "^5.13.7",
    "gatsby-plugin-image": "^3.13.1",
    "gatsby-plugin-postcss": "^6.13.1",
    "gatsby-plugin-react-svg": "^3.3.0",
    "gatsby-source-contentful": "^8.13.2",
    "gatsby-source-filesystem": "^5.13.1",
    "gatsby-transformer-json": "^5.13.1",
    "postcss": "^8.4.41",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@appsfactory/cypress-audit": "^3.0.4",
    "@babel/preset-typescript": "^7.24.7",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.7.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "babel-jest": "^29.7.0",
    "babel-preset-gatsby": "^3.13.2",
    "contentful-import": "^9.4.56",
    "css-mediaquery": "^0.1.2",
    "cypress": "^13.14.2",
    "cypress-terminal-report": "^6.1.2",
    "cypress-visual-regression": "^5.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-css": "^6.0.1",
    "react-test-renderer": "^18.3.1",
    "start-server-and-test": "^2.0.5",
    "ts-jest": "^29.2.4",
    "typescript": "^5.5.4"
  }
}
